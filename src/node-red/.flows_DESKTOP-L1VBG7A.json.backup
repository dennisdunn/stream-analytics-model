    {
        "id": "263fb80a.1f2298",
        "type": "function",
        "z": "85a05cda.a32ed",
        "name": "forecast",
        "func": "const target = .75;\n\nvar m = context.global.moment;\nvar now = m(msg.timestamp);\nvar forecast = m((target - msg.payload.equation[1])/msg.payload.equation[0]);\nvar delta = m.duration(forecast.diff(now)).asDays();\n\ndelta = Math.floor(delta);\n\nif(delta < 0)return null;\n\nmsg.payload = +forecast;\nmsg.topic = 'forecast';\nreturn [{timestamp:msg.timestamp, payload:delta},msg];",
        "outputs": 2,
        "noerr": 0,
        "x": 160,
